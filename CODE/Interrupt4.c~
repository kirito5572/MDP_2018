 #include <mega128.h>
 unsigned char FNDC[4] = {0x66, 0x6d, 0x7d, 0x07};
 void main(void) {
    DDRB = 0xff;
    DDRE = 0x00;
    DDRF = 0xf0;
    PORTF = 0x70;
    SREG = 0x80;
    EICRB = 0b10101010;
    EIMSK = 0xf0;
    while(1) {
        
    }   
 }
 interrupt [EXT_INT4] void ex_int4(void) { 
 PORTB = FNDC[0];
 }
 interrupt [EXT_INT5] void ex_int5(void) {
 PORTB = FNDC[1];
 }
 interrupt [EXT_INT6] void ex_int6(void) { 
 PORTB = FNDC[2];
 }
 interrupt [EXT_INT7] void ex_int7(void) { 
 PORTB = FNDC[3];
 }